<%= simple_form_for(@want_course) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :course_number %>
    <%= f.input :course_name %>
    <%= f.input :course_section %>
    <%= f.input :trade_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
